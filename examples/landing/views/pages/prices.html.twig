{% extends 'base.html.twig' %}

{% block link_class_prices %}active{% endblock %}

{% block app_container %}
    <section id='prices'>
        <div class='panel panel-success'>
            <div class='panel-heading'>
                <h4 class='panel-title'>No worry, I'm just kidding...</h4>
            </div>
            <div class='panel-body'>
                <h5>I am very proud to trust in <strong>Open-Source</strong> projects and technologies</h5>
                <p>You are <span class='text-success'>free to use it</span> in your projects, as a developer or even a big company.</p>
